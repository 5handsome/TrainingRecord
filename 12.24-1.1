		printf("%d ",a[i]) ;
}
void sou_sort(int a[], int left, int right)
{
    if (left >= right)
        return;
    if (right - left + 1 < 10)
    {
        chasort(a, left, right);
        return;
    }else
	{
	srand(time(NULL));
    int i = left, j = right,pivot=rand()%(right-left)+left;
    swap(a[left],a[pivot]);
    int key=a[left];
    while (i < j)
    {
    	while(i<j&&key<=a[j])
		{
			j--;
		}
    	while(i<j&&key>=a[i])
		{
			i++;
		}
			swap(a[i],a[j]);
    }
    swap(a[j],a[left]);
    sou_sort(a,left,j-1);
    sou_sort(a,j+1,right);
	}
}
void swap(int *b,int *c)
{
	int temp;
	temp=*b;
	*b=*c;
	*c=temp;
}
void chasort(int a[],int left,int right)
{
    for (int i = left + 1; i <= right; i++)
        for (int j = i; j > 0 && a[j] < a[j-1]; j--)
            swap(a[j], a[j-1]);
}
