#include<iostream>
#include<cmath>
using namespace std;
int main(void)
{
	int a,j,n,S[101]={0},I[101]={0},R[101]={0};
	double b=0.0,r=0.0;
	cin >> S[0] >> I[0] >> n;
	cin >> b >> r;
	for(int i=1;i<=n;i++)
	{
		a=ceil(b*S[i-1]*I[i-1]);
		j=ceil(r*I[i-1]);
		if(a>=S[i-1])
		{
            S[i]=0,I[i]=I[i-1]+S[i-1]-j,R[i]=R[i-1]+j;
        }
        else{
		R[i]=R[i-1]+j;
		I[i]=ceil(I[i-1]+a-j);
		S[i]=S[i-1]-a;
		}
	}
	cout << S[n] << " " << I[n] << " " << R[n];
}
